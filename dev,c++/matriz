// Matrices
#include <iostream>
usando namespace std;
int main () {
    int matriz [3] [3];
    int fila, col;
    for (fila = 0; fila <3; fila ++) {
       para (col = 0; col <3; col ++) {
         cout << "matriz [" << fila << "] [";
         cout << col << "]";
         cin >> matriz [fila] [col]; 
       }
    }
    cout << endl << "Hemos acabado";
    return 0;
}
