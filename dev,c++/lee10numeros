/ * Este programa sirve para leer por teclado 10 números enteros y devolver
el mayor * /
#include <iostream>

int main () {
    // Definición de variables
    int cont;
    int numero;
    int alcalde;
    int salir;
    // Bucle de tipo para cont = cont + 1 es lo mismo cont ++
    // for (cont = 10; cont> = 0; cont -) {....
    std :: cout << "Dime un numero entero:";
    std :: cin >> numero;
    mayor = numero;
    para (cont = 1; cont <10; cont ++) {
          std :: cout << "Dime un numero entero:";
          std :: cin >> numero;
          if (numero> mayor) {
            mayor = numero;
          }
    }
    std :: cout << "El mayor es:" << mayor;
    std :: cout << "To tecla cualquier tecla";
    std :: cin >> salir;
    return 0;
 
}
